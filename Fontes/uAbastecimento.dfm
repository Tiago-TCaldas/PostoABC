  object qrConsulta: TFDQuery
    Connection = DataModu.ConexaoBD
    SQL.Strings = (
      'SELECT B.IDTANQUE'
      '      ,B.IDBOMBA'
      '      ,B.IDCOMBUSTIVEL'
      '      ,C.DESCRICAO'
      '      ,C.VALOR'
      '      ,C.PERCENTUALIMPOSTO'
      '  FROM bombacombustivel B'
      ' INNER JOIN COMBUSTIVEL C'
      '    ON B.IDCOMBUSTIVEL = C.IDCOMBUSTIVEL'
      ' WHERE B.IDTANQUE = :idTanque'
      '   AND B.IDBOMBA = :IdBomba'
      '   AND C.IDCOMBUSTIVEL = :IdCombustivel')
    Left = 497
    Top = 120
    ParamData = <
      item
        Name = 'IDTANQUE'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'IDBOMBA'
        DataType = ftInteger
        ParamType = ptInput
      end
      item
        Name = 'IDCOMBUSTIVEL'
        DataType = ftInteger
